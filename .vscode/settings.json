// Place your settings in this file to overwrite default and user settings.
{
  "editor.rulers": [
    120
  ],
  "files.exclude": {
    "dist": true,
    "spec/**/*.js": true,
    "spec/**/*.js.map": true,
    "node_modules": true,
    "coverage": true,
    "temp": true,
    "*.tgz": true,
    ".serverless": true,
    ".webpack": true,
    ".nyc_output": true
  },
  "jscs.enable": false,
  "jshint.enable": false,
  "mocha.files.glob": "dist/**/*.test.js",
  "mocha.env": {
    "__TEST__": true
  },
  "mocha.options": {},
  "tslint.enable": true,
  "tslint.ignoreDefinitionFiles": false,
  "tslint.exclude": "**/node_modules/**/*.ts",
  "search.exclude": {
    "node_modules": true,
    "dist/**": true,
    "spec/**/*.js": true,
    "spec/**/*.js.map": true,
    "coverage/**": true,
    "temp": true,
    "*.tgz": true,
    ".serverless": true,
    ".webpack": true,
    ".nyc_output": true
  },
  "npm-intellisense.scanDevDependencies": true,
  "typescript.format.enable": true,
  "typescript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": false,
  "typescript.tsdk": "./node_modules/typescript/lib",
  "typescriptHero.resolver.pathStringDelimiter": "\"",
  "typescriptHero.restartDebugger.active": false
}
